{
  "name": "Stargazer",
  "version": "1.0.0",
  "description": "video app",
  "author": "SteagsInc",
  "build": {
    "productName": "Stargazer",
    "appId": "com.SteagsInc.Stargazer",
    "files": [
      "./src/**/*",
      "**/mpv/**/*",
      "./build/**/*",
      "public/main.js",
      "package.json"
    ],
    "asar": true,
    "asarUnpack": [
      "**/mpv/**/*"
    ],
    "npmRebuild": false,
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    },
    "extends": null
  },
  "homepage": "./",
  "main": "public/main.js",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "bootstrap": "^4.6.0",
    "concurrently": "^6.0.0",
    "cross-env": "^7.0.3",
    "electron-is-dev": "^2.0.0",
    "electron-json-storage": "^4.4.0",
    "electron-json-storage-sync": "^1.1.1",
    "fluent-ffmpeg": "^2.1.2",
    "fs": "0.0.1-security",
    "mpv.js": "^0.3.0",
    "prebuild": "^10.0.1",
    "react": "^16.14.0",
    "react-bootstrap": "^1.5.2",
    "react-dom": "^16.14.0",
    "react-html5video": "^2.5.1",
    "react-loader-spinner": "^4.0.0",
    "react-redux": "^7.2.3",
    "react-scripts": "5.0.1",
    "react-select": "^3.2.0",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "torrent-name-parser": "^0.6.5",
    "video.js": "^7.11.4",
    "wait-on": "^5.3.0"
  },
  "devDependencies": {
    "electron": "^9.0.0",
    "electron-builder": "^22.10.5"
  },
  "scripts": {
    "react-start": "react-scripts start",
    "start": "concurrently \"cross-env BROWSER=none yarn react-start\" \"wait-on http://localhost:3000 && electron .\"",
    "react-build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron-start": "electron .",
    "electron-build": "electron-builder",
    "buildlinux": "yarn react-build && yarn electron-build --linux",
    "buildwin": "yarn react-build && yarn electron-build --win --x64 --ia32",
    "builddir": "yarn react-build && yarn electron-build --dir",
    "release": "yarn react-build && electron-builder --publish=always"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
